{
    "libpath": "site/assets/dist/lib",
    "grouppath": "site/assets/dist/group",
    "pagepath":"site/assets/dist/page",
    "webroot":"site",
    "dst": "parsed.json",
    "min":true,
    "clean":false,
    "libs": {
        "jquery": {
            "name":"jquery",
            "bower": true
        },
        "bootstrap": {
            "name":"bootstrap",
            "bower": true
        },
        "date-time": {
            "name":"date-time",
            "bower": false,
            "files": ["site/assets/lib/date-time/*.*"],
            "dependencies": ["jquery", "bootstrap"]
        }
    },
    "groups": {
        "base": {
            "name":"base",
            "libs": ["jquery", "bootstrap"],
            "files":["run.js"]
        }
    },
    "pages": {
        "act": {
            "name":"act",
            "groups": ["base"],
            "libs": ["date-time"],
            "files":["run.js"]
        },
        "home":{
            "name":"home",
            "libs":["jquery"]
        }
    }
}