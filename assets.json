{
    "foo": "bar",
    "libpath": "site/assets/dist/lib",
    "grouppath": "site/assets/dist/group",
    "pagepath": "site/assets/dist/page",
    "webroot": "site",
    "dst": "parsed.json",
    "min": true,
    "clean": false,
    "hash": true,
    "oss": "config/oss.json",
    "libs": {
        "jquery": {
            "name": "jquery",
            "files": [
                "site/assets/lib/jquery/jquery.js"
            ]
        },
        "bootstrap": {
            "name": "bootstrap",
            "files": [
                "site/assets/lib/bootstrap/css/bootstrap.css",
                "site/assets/lib/bootstrap/js/bootstrap.js"
            ],
            "dependencies": [
                "jquery"
            ]
        },
        "date-time": {
            "name": "date-time",
            "files": ["site/assets/lib/date-time/*.*"],
            "dependencies": [
                "jquery",
                "bootstrap"
            ]
        }
    },
    "groups": {
        "base": {
            "name": "base",
            "libs": [
                "jquery",
                "bootstrap"
            ],
            "files": ["run.js"]
        },
        "dt": {
            "name": "dt",
            "libs": ["date-time"]
        }
    },
    "pages": {
        "act": {
            "name": "act",
            "groups": [
                "base",
                "dt"
            ],
            "libs": ["umeditor"]
        },
        "home": {
            "name": "home",
            "libs": ["jquery"],
            "templates": [
                "site/assets/home/nav.html",
                "site/assets/home/detail.html"
            ],
            "templatePath": "site/assets/home/tmpl.js",
            "files": [
                "site/assets/home/*.js"
            ]
        }
    }
}